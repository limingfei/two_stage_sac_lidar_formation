<launch>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <remap from="cmd_vel" to="robot2_cmd_vel" />
        <rosparam file="$(find full_vehicle_description)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find full_vehicle_description)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find full_vehicle_description)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find full_vehicle_description)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find full_vehicle_description)/param/base_local_planner_params.yaml" command="load" />
    </node>

</launch>
