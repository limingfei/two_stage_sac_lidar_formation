    <plugin filename="libnexus_ros_force_based_move.so" name="robot1nexus_ros_force_based_move">
      <commandTopic>robot1_cmd_vel</commandTopic>
      <odometryTopic>robot1_odom</odometryTopic>
      <odometryFrame>robot1_odom</odometryFrame>
      <odometryRate>25.0</odometryRate>
      <robotBaseFrame>robot1_base_footprint</robotBaseFrame>
      <cmdVelTimeOut>0.25</cmdVelTimeOut>
      <publishOdometryTf>0</publishOdometryTf>
      <yaw_velocity_p_gain>100.0</yaw_velocity_p_gain>
      <x_velocity_p_gain>1000.0</x_velocity_p_gain>
      <y_velocity_p_gain>1000.0</y_velocity_p_gain>
      <max_x_velocity>1.5</max_x_velocity>
      <max_y_velocity>1.5</max_y_velocity>
      <max_yaw_velocity>1.5</max_yaw_velocity>
    </plugin>
